<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1d1f29"
    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="-62dp"
            tools:layout_editor_absoluteY="0dp">

            <Button
                android:id="@+id/albedoBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="@string/albedo" />

            <Button
                android:id="@+id/amberBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="@string/amber" />

            <Button
                android:id="@+id/barbaraBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="@string/barbara" />

            <Button
                android:id="@+id/beidouBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Beidou" />

            <Button
                android:id="@+id/bennetBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Bennet" />

            <Button
                android:id="@+id/chongyunBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Chongyun" />

            <Button
                android:id="@+id/dilucBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Diluc" />

            <Button
                android:id="@+id/dionaBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Diona" />

            <Button
                android:id="@+id/fischlBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Fischl" />

            <Button
                android:id="@+id/ganyuBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Ganyu" />

            <Button
                android:id="@+id/jeanBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Jean" />

            <Button
                android:id="@+id/kaeyaBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Kaeya" />

            <Button
                android:id="@+id/keqingBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Keqing" />

            <Button
                android:id="@+id/kleeBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Klee" />

            <Button
                android:id="@+id/lisaBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Lisa" />

            <Button
                android:id="@+id/monaBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Mona" />

            <Button
                android:id="@+id/ningguangBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Ningguang" />

            <Button
                android:id="@+id/noelleBtn"
