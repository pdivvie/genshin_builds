            <Button
                android:id="@+id/qiqiBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Qiqi" />

            <Button
                android:id="@+id/razorBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Razor" />

            <Button
                android:id="@+id/sucroseBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Sucrose" />

            <Button
                android:id="@+id/tartagliaBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Tartaglia" />

            <Button
                android:id="@+id/travelerBtn"
                android:layout_width="match_parent"
