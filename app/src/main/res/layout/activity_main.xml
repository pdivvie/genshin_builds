                android:backgroundTint="#222431"
                android:text="Traveler" />

            <Button
                android:id="@+id/ventiBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Venti" />

            <Button
                android:id="@+id/xianglingBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Xiangling" />

            <Button
                android:id="@+id/xiaoBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Xiao" />

            <Button
                android:id="@+id/xinqiuBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Xinqiu" />

            <Button
                android:id="@+id/xinyanBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Xinyan" />

            <Button
                android:id="@+id/zhongliBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#222431"
                android:text="Zhongli" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>